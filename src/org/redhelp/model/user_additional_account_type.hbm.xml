<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.redhelp.model.UserAdditionalAccountTypeModel" table="USER_ADDITIONAL_ACCOUNT_TYPE">
        <id name="uaat_id" type="java.lang.Long">
            <column name="UAAT_ID" />
        </id>
        <property name="name" type="string">
            <column name="NAME" length="50" />
        </property>
        
    </class>
</hibernate-mapping>